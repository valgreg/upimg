{
    "name": "valgreg/upimg",
    "description": "Simple Repository",
    "keywords": ["package", "repository", "generator", "static", "composer"],
    "homepage": "https://github.com/valgreg/upimg",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "ValGreg225",
        "email": "gvallejo@free.fr",
        "homepage": "https://gregoryvallejo.fr"
    }],
    "support": {
        "source": "https://github.com/valgreg/upimg"
    },
    "bin": ["bin/satis"],
    "require": {
        "php": "^7.3||^8.0",
        "composer/composer": "^1.9",
        "symfony/console": "^4.4||^5.0",
        "twig/twig": "^2.12||^3.0"
    },
    "autoload": {
        "psr-4": { "Valgreg\\Upimg\\": "src" }
    },
    "require-dev": {
        "ext-json": "*",
        "friendsofphp/php-cs-fixer": "^2.15",
        "mikey179/vfsstream": "^1.6.7",
        "phpunit/phpunit": "^8.2||^9.0"
    },
    "autoload-dev": {
        "psr-4": { "Composer\\Satis\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
